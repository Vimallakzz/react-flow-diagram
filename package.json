    "babel-eslint": "^8.0.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "5.x",
    "eslint-plugin-react": "^7.2.1",
    "flow-bin": "^0.57.3",
    "flow-typed": "^2.1.5",
    "nwb": "^0.23.0",
